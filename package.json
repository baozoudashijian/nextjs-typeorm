{
   "name": "nextjs-typeorm",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "dev": "next dev",
      "build": "next build",
      "start": "next start",
      "typeorm:start": "node ./dist/index.js",
      "typeorm:build": "babel ./src --out-dir dist --extensions .ts,.tsx",
      "migration:run": "typeorm migration:run -d ./dist/data-source.js",
      "migration:revert": "typeorm migration:revert -d ./dist/data-source.js"
   },
   "dependencies": {
      "@babel/cli": "^7.17.10",
      "next": "12.1.6",
      "pg": "^8.4.0",
      "react": "18.1.0",
      "react-dom": "18.1.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.6"
   },
   "devDependencies": {
      "@babel/core": "^7.18.2",
      "@babel/plugin-proposal-decorators": "^7.18.2",
      "@babel/preset-env": "^7.18.2",
      "@types/node": "^16.11.10",
      "@types/react": "18.0.9",
      "@types/react-dom": "18.0.5",
      "babel-plugin-transform-typescript-metadata": "^0.3.2",
      "eslint": "8.16.0",
      "eslint-config-next": "12.1.6",
      "typescript": "4.5.2"
   }
}
